<!DOCTYPE html>

<html>
	<head>
	  <title>VeterinaryClinic</title>
	  
	 
	  <%= render 'layouts/stylesheets' %>	
   	</head>
	<body>  
  <div class="container">
    <%= render 'layouts/header' %> 
      
    <section class="round">
      
     
        <%- flash.each do |name, msg| -%> 
          <%= content_tag :div, msg, :id => "flash_#{name}" %>       
        <%- end -%>
        
        <%= yield %>
       
    </section>
    <%= render 'layouts/footer'%>
  </div>
	</body>
</html>
