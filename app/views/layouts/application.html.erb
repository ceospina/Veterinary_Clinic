<!DOCTYPE html>
<html>
	<head>
	  <title>VeterinaryClinic</title>
	  <%= javascript_include_tag :defaults%>
    <%= csrf_meta_tag %>
	  <%= render 'layouts/stylesheets' %>	

	</head>
	<body>  
  <div class="container">
    <%= render 'layouts/header' %> 
      
    <section class="round">
        <%- flash.each do |name, msg| -%> 
          <%= content_tag :div, msg, :id => "flash_#{name}" %>
        <%- end -%>
        
        <%= yield %>
    </section>
    
    <%= render 'layouts/footer'%>
      
    </div>



	</body>
</html>
